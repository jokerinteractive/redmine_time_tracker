<% content_for :header_tags do %>
    <%= javascript_include_tag 'time_tracker2', :plugin => 'redmine_time_tracker' %>
    <%= javascript_include_tag 'validators', :plugin => 'redmine_time_tracker' %>
    <%= javascript_include_tag 'field_updater', :plugin => 'redmine_time_tracker' %>
<% end %>
<%= javascript_tag render :partial => 'tt_overview/time_tracker', :formats => [:js] %>

<% include_calendar_headers_tags %>
<%= patch_date_picker %>
<% html_title l(:time_tracker_overview_title) %>

<% if help.permission_checker([:tt_log_time], {}, true) %>
<%= render 'time_trackers/time_tracker_control' %>
<% end %>

<%= render 'tt_overview/logs_ov' %>
<%= render 'tt_overview/bookings_ov' %>

<%= context_menu tt_overview_context_menu_path  %>

<%= javascript_tag render :partial => 'highlight_bookings', :formats => [:js], :locals => {:lab_ids => params[:tl_labids]} %>